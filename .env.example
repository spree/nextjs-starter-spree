# Spree API Configuration
NEXT_PUBLIC_SPREE_API_URL=http://localhost:3000
NEXT_PUBLIC_SPREE_API_KEY=your_publishable_api_key

# Store defaults (should match your Spree store settings)
# These are used by the proxy for initial redirects before API data is loaded
# Set these to your store's default_country_iso and default_locale
NEXT_PUBLIC_DEFAULT_COUNTRY=us
NEXT_PUBLIC_DEFAULT_LOCALE=en
